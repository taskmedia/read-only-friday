    "@vercel/ncc": "^0.34.0",
    "eslint": "^8.27.0",
    "eslint-plugin-github": "^4.4.1",
    "eslint-plugin-jest": "^27.1.5",
    "jest": "^27.2.5",
    "js-yaml": "^4.1.0",
    "prettier": "2.7.1",
    "ts-jest": "^27.1.2",
