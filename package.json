    "@actions/core": "^1.11.1"
  },
  "devDependencies": {
    "@types/node": "^22.13.9",
    "@typescript-eslint/parser": "^8.25.0",
    "@vercel/ncc": "^0.38.3",
    "eslint": "^9.21.0",
    "eslint-plugin-github": "^5.1.8",
    "eslint-plugin-jest": "^28.11.0",
    "jest": "^29.7.0",
    "js-yaml": "^4.1.0",
    "prettier": "3.5.3",
    "ts-jest": "^29.2.6",
    "typescript": "^5.8.2"
  }
