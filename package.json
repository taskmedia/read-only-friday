{
  "name": "read-only-friday",
  "version": "24.0.8",
  "private": true,
  "type": "module",
  "description": "Fails if it is friday - do not change anything on friday!",
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "bundle": "npm run format && npm run package",
    "format": "prettier --write .",
    "format-check": "prettier --check .",
    "lint": "eslint .",
    "local-action": "node dist/index.js",
    "package": "rimraf ./dist && rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "package:watch": "npm run package -- --watch",
    "test": "jest",
    "all": "npm run format && npm run lint && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^3.0.0"
  },
  "devDependencies": {
    "@github/local-action": "^3.2.0",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/node": "^25.3.0",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "eslint": "^10.0.1",
    "eslint-plugin-jest": "^29.15.0",
    "jest": "^30.2.0",
    "prettier": "^3.8.1",
    "rimraf": "^6.0.1",
    "rollup": "^4.59.0",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.56.0"
  },
  "overrides": {
    "minimatch": "^10.2.1",
    "glob": "^11.0.0"
  }
}
