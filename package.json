    "@typescript-eslint/parser": "^8.55.0",
    "@vercel/ncc": "^0.38.4",
    "eslint": "^10.0.0",
    "eslint-plugin-github": "^6.0.0",
    "eslint-plugin-jest": "^29.15.0",
    "jest": "^30.2.0",
    "js-yaml": "^4.1.1",
    "prettier": "3.8.1",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3"
