  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@typescript-eslint/parser": "^8.48.1",
    "@vercel/ncc": "^0.38.4",
    "eslint": "^9.39.1",
    "eslint-plugin-github": "^6.0.0",
    "eslint-plugin-jest": "^29.2.1",
    "jest": "^30.2.0",
    "js-yaml": "^4.1.1",
    "prettier": "3.7.3",
    "ts-jest": "^29.4.5",
    "typescript": "^5.8.3"
  }
